{
    "name": "joelwalls/picoyplaca",
    "description": "This simple package predicts if a given license place can be on the road at a certain date and time based on Quito restrictions.",
    "keywords": [
        "pico",
        "placa",
        "pico y placa",
        "ecuador",
        "quito"
    ],
    "homepage": "https://github.com/joelwalls/picoyplaca",
    "license": "MIT",
    "authors": [
        {
            "name": ":Joel Paredes",
            "email": "admin@joelwalls.com",
            "homepage": "http://joelwalls.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.3"
    },
    "autoload": {
        "psr-4": {
            "JoelWalls\\PicoYPlaca\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoelWalls\\PicoYPlaca\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
